#!/usr/bin/env bash
# Write a Bash script that loops from 1 to 10 and using while if elf else
COUNTER=1
while [ $COUNTER -lt 11 ]
do
    if [ $COUNTER = 4 ]
        then
	        echo "bad luck"
		    elif [ $COUNTER = 8 ]
		        then
			        echo "good luck"
				    else
				            echo "Best School"
					        fi
						        (( COUNTER+=1 ))
							done
							